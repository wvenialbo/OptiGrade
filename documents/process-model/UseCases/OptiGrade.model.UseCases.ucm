    <UseCases>
      <UseCase UniqueID="83c2c80e-6970-4c11-b7b3-df3fce5804d1" Name="Sign up" ID="1" Prefix="UC">
        <Attributes>
          <Description>The visitor has the ability to join the system as a user.</Description>
          <Notes>In our initial scenarios, the relevant UI must be browser-based.</Notes>
          <RelatedDocuments />
        </Attributes>
        <Steps>
          <Step UniqueID="97962982-448d-4deb-aaf1-01c1e96e3005" Name="1" ID="1" Prefix="">
            <Description>The visitor selects the 'Sign up' link</Description>
            <ChildID>-1</ChildID>
            <Type>Default</Type>
          </Step>
          <Step UniqueID="5cdc3e0a-ddf1-472b-bbf3-1f50b20578d2" Name="2" ID="2" Prefix="">
            <Description>The visitor is shown a screen where the user can create a user name.</Description>
            <ChildID>-1</ChildID>
            <Type>Default</Type>
          </Step>
          <Step UniqueID="12d36a2b-92fc-4051-8b64-444ce730c2f5" Name="3" ID="3" Prefix="">
            <Description>The visitor must add his email (mandatory), password (mandatory) and location.</Description>
            <ChildID>-1</ChildID>
            <Type>Default</Type>
          </Step>
          <Step UniqueID="0d04a179-0e8a-4abc-837b-87383076ff9e" Name="4" ID="4" Prefix="">
            <Description>The visitor must check the acceptance of the service provider condition of terms.</Description>
            <ChildID>-1</ChildID>
            <Type>Default</Type>
          </Step>
          <Step UniqueID="86609f8a-6828-498f-8eeb-f16d6aceffc7" Name="5" ID="5" Prefix="">
            <Description>The system sends a confirmation email message with an activation hyperlink to the user based on the email submitted.</Description>
            <ChildID>-1</ChildID>
            <Type>Default</Type>
          </Step>
        </Steps>
        <OpenIssues />
        <ActiveActors />
        <HistoryItems />
        <Prose />
        <Preconditions>1. User has a valid email account address.</Preconditions>
        <Postconditions>1. User is registered.
2. User is assigned the role 'Visitor'.</Postconditions>
